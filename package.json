{
  "name": "calculator--",
  "homepage": "https://https://iszachariah.github.io/scientific-calculator/",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "NODE_ENV=production node server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build --outDir dist/client",
    "build:server": "tsc && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "listhen": "^1.0.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.4.3",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "tailwindcss-text-fill-stroke": "^1.1.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
